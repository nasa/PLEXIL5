  state time           () = 3 : int;
  state time           () = 4 : int;
  state time           () = 5 : int;
  state time           () = 6 : int;
  state time           () = 7 : int;
  state time           () = 8 : int;
  state time           () = 9 : int;
  state time           () = 10 : int;
  state time           () = 11 : int;
  state time           () = 12 : int;
  state time           () = 13 : int;
  state time           () = 14 : int;
  state time           () = 15 : int;
  state time           () = 16 : int;
  state time           () = 17 : int;
  state time           () = 18 : int;
  state time           () = 19 : int;
  simultaneous {
    command-sent-to-system SetVel (-100000:real,-100000:real,-100000:real);
    state time           () = 20 : int;
  }
  state time           () = 21 : int;
  state time           () = 22 : int;
  simultaneous {
    state trafficConflict      () = false : bool;
