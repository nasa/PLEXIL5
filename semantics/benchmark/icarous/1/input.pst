// Simulating SPEED resolution
// - 2nd condition

initial-state {
  state time                 () = 0 : int;
  state nextWP               () = 0 : int;
  state trafficConflict      () = true : bool;
  state trafficSpeedConflict () = true : bool;
  state preferredSpeed       () = -100 : real;
  state velocity             () = (0,0,0) : real-array;
}


