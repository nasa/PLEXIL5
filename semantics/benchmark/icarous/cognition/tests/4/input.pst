initial-state {
  state time           () = 0 : int;
  state inConflict     () = true : bool;
}

script {
  state time           () = 1 : int;
  state time           () = 2 : int;
  state time           () = 3 : int;
  simultaneous {
    state time           () = 4 : int;
    state inConflict     () = false : bool;
  }
}
