      executing(
        [ environment : mtenvironment ]
        [ nodes :
            < A     : list  | inv: const(val(false)),
                              exitc: const(unknown) >
            < B . A : empty | status: executing,
                              active: false,
                              inv: const(unknown),
                              post: const(unknown),
                              endc: const(unknown),
                              exitc: const(unknown) > ]
        [ microacts : setOutcome(B . A,failure(parentFailed)),
                      setStatus(B . A,finished),
                      logTransition(B . A, executing, finished, 3) ]
        [ memory : none ]
      )
    end

    assert "Executing empty node follows path 4 (executing--empty--04)" from
      executing(
        [ environment : mtenvironment ]
        [ nodes :
            < A     : list  | inv: const(unknown),
                              exitc: const(unknown) >
