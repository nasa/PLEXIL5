                      setStatus(B . A,iterationEnded),
                      logTransition(B . A, executing, iterationEnded, 6) ]
        [ memory : none ]
      )
    end

    assert "Executing empty node follows path 7 (executing--empty--07)" from
      executing(
        [ environment : mtenvironment ]
        [ nodes :
            < A     : list  | inv: const(unknown),
                              exitc: const(unknown) >
            < B . A : empty | status: executing,
                              active: true,
                              inv: const(unknown),
                              post: const(unknown),
                              endc: const(unknown),
                              exitc: const(unknown) > ]
        [ microacts : mtactions ]
        [ memory : none ]
      )
    reaches
      executing(
        [ environment : mtenvironment ]
        [ nodes :
            < A     : list  | inv: const(unknown),
                              exitc: const(unknown) >
            < B . A : empty | status: executing,
