                      setStatus(B . A,iterationEnded),
                      logTransition(B . A, executing, iterationEnded, 5) ]
        [ memory : none ]
      )
    end

    assert "Executing empty node follows path 6 (executing--empty--06)" from
      executing(
        [ environment : mtenvironment ]
        [ nodes :
            < A     : list  | inv: const(unknown),
                              exitc: const(unknown) >
            < B . A : empty | status: executing,
                              active: true,
                              inv: const(unknown),
                              post: const(val(true)),
                              endc: const(val(true)),
                              exitc: const(unknown) > ]
        [ microacts : mtactions ]
